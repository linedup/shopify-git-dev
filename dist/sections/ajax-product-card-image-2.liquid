<div id="ajax-product-card-image-2">
  {% for image in product.images limit:1 offset:1 %}
    {% liquid
      assign altTag = image.alt
      if altTag == blank
        assign altTag = product.title
      endif
      render 'image' with image_src: image.src, alt_tag: altTag
    %}
  {% endfor %}
</div>